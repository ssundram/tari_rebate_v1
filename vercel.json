{
  "version": 2,
  "buildCommand": "echo 'No build needed for static files'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/",
      "dest": "/invoice-upload.html"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|otf))",
      "dest": "/$1"
    }
  ]
}